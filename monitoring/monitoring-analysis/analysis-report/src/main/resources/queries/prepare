{
    "size":%d,
    "query":{
        "bool":{
            "must":[
                {"term":{"%sapplication.id.raw":"%s"}},
                {"term":{"%stype.raw":"fulltext"}}
            ],
            "must_not":{
                "exists":{"field":"%sprocess"}
             },
            "filter": [
                {
                    "range":{
                        "%stimeStamp":{
                            "gte":%d,
                            "lte":%d,
                            "format":"epoch_millis"
                        }
                    }
                }
            ]
        }
    }
}